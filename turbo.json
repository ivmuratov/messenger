{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["eslint.config.ts", "tsconfig.base.json"],
  "globalPassThroughEnv": ["WEB_PORT", "RCT_METRO_PORT"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "dist-native/**", ".expo/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "typecheck": {},
    "format": {
      "cache": false
    },
    "format:check": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
